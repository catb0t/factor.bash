include frext

assert_eq(
  Hash(
    :change_me => true,
    :dont_change_me => false,
    :here => 1
  ).clobber_existing_by(
    Hash( :change_me => { :new_val }, :dont_change_me => { :okay_i_wont }, :here => { :there } )
  ),

  Hash( :change_me => :new_val, :dont_change_me => false, :here => :there )
)
