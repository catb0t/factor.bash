class X {
  -> m (String x, Bool f = false, *args) {
    "x: #{x}, f: #{f}, args: #{args}"
  }
  -> u (String x, Bool f = false) {
    "x: #{x}, f: #{f}"
  }
  -> t (Bool f = false, *args) {
    "f: #{f}, args: #{args}"
  }
}
func f (n, r = 1, *y) { [n, r, y...] }

say f(:ABC, 123)
say X().u(:abc)
say X().m(:abc, f: true, :def)
say X().t(:abc)
say X().m(:abc, :def)
